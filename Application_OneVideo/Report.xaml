                    <Canvas Height="74" Canvas.Top="150" Width="450" Background="#FFECEAEA">
                        <Label Content="状态" Canvas.Left="22" Canvas.Top="10" FontSize="36"/>
                        <Label x:Name="label1" Content="否" Canvas.Left="160" Canvas.Top="10" FontSize="36"/>
